{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "animal_type": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "age": {
      "type": "number"
    },
    "has_tail": {
      "type": "boolean"
    },
    "colors": {
      "type": "array",
      "items": [
        {
          "type": "string"
        },
        {
          "type": "string"
        },
        {
          "type": "string"
        }
      ]
    },
    "friends": {
      "type": "object",
      "properties": {
        "count": {
          "type": "number"
        }
      },
      "required": [
        "count"
      ]
    }
  },
  "required": [
    "animal_type",
    "name",
    "age",
    "has_tail",
    "colors",
    "friends"
  ]
}


